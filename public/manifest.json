{
    "manifest_version": 3,

    "version": "0.0.1",

    "name" : "Template",
    "description" :"Chrome extension template",
    "icons": {},
    "action": {
        "default_popup": "build/html/popup.html"
    },
    "content_scripts" : [{
        "matches": ["<all_urls>"],
        "js": [
            "build/js/content.js"
        ]
    }]
}